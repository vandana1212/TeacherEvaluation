  	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>SecondServlet</servlet-name>
  	<servlet-class>com.teacher.evaluation.controller.infocontrol</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SecondServlet</servlet-name>
  	<url-pattern>/third</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ThirdServlet</servlet-name>
  	<servlet-class>com.teacher.evaluation.controller.recommendcontrol</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ThirdServlet</servlet-name>
  	<url-pattern>/rec</url-pattern>
